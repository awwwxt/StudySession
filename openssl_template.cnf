[ req ]
default_bits       = 4096
default_keyfile    = key.pem
default_md         = sha256
default_days       = 365
prompt             = no
distinguished_name = dn
req_extensions     = req_ext

[ dn ]
C  = RU
ST = State
L  = Astrakhan
O  = Organization
CN = localhost

[ req_ext ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1 = localhost
IP.1 = 127.0.0.1
